<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:backgroundTint="@color/white"
    tools:context=".Home">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="145dp"
        android:layout_height="27dp"
        android:text="@string/fechaE"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="223dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="239dp"
        android:layout_height="52dp"
        android:text="@string/app_name"
        android:textAlignment="center"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="16dp" />


    <TextView
        android:id="@+id/FechaI"
        android:layout_width="145dp"
        android:layout_height="27dp"
        android:text="@string/fechaI"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="121dp" />

    <Button
        android:id="@+id/reservarButton"
        android:layout_width="161dp"
        android:layout_height="56dp"
        android:backgroundTint="@color/naranja"
        android:gravity="center"
        android:onClick="reservar"
        android:text="@string/reservar"
        tools:ignore="TextContrastCheck"
        tools:layout_editor_absoluteX="125dp"
        tools:layout_editor_absoluteY="589dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@color/gris"
        app:itemIconTint="@drawable/selector"
        app:menu="@menu/bottom_nav_menu"
        tools:layout_editor_absoluteY="653dp" />

    <Button
        android:id="@+id/buttonIngreso"
        android:layout_width="166dp"
        android:layout_height="53dp"
        android:backgroundTint="@color/azul"
        android:onClick="fechaIngreso"
        android:text="@string/ingreso"
        android:textColorLink="@color/clear"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="148dp" />

    <Button
        android:id="@+id/buttonSalida"
        android:layout_width="166dp"
        android:layout_height="53dp"
        android:backgroundTint="@color/azul"
        android:onClick="fechaSalida"
        android:text="@string/salida"
        android:textColorLink="@color/clear"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="250dp" />

    <TextView
        android:id="@+id/textViewIngreso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="141dp" />

    <TextView
        android:id="@+id/textViewSalida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="251dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>